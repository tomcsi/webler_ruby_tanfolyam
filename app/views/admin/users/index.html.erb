<h1>Users</h1>

<%= link_to "New", new_admin_user_path %>

<table border="1">
<% @users.each do |u| %>
<tr>
<td><%= u.name %></td>
<td><%= u.birthdate %></td>
<td><%= link_to "Edit", edit_admin_user_path(u.id) %> <%= link_to "Delete", admin_user_path(u.id), method: :delete %></td>
</tr>
<% end %>
</table>


